#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <string.h>


int main(){
setlocale(LC_ALL,"PORTUGUESE_BR");                                                  
 
  char str1[100],str2[100];
 
 
  printf("Informe uma palavra:");
  scanf("%s",&str1);
  printf("Informe uma palavra:");
  scanf("%s",&str2);
 
  int result=strcmp(str1, str2);
 
  if(result==0){
    printf("Strings iguais");
    }else{
    printf("Strings diferentes");
  }
