#include <stdio.h>
#include <stdlib.h>
#include <locale.h>

int main(){
 
setlocale(LC_ALL,"PORTUGUESE_BR");                                                  
 
  int num;
 
  printf("Informe um numero:");
  scanf("%i",&num);
 
  if(num<10){
    printf("O numero é menor que 10");
    }else{
    printf("O numero é maior que 10");
  }
}
