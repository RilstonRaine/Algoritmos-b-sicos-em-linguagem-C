#include <stdio.h>
#include <stdlib.h>
#include <locale.h>


int main(){
setlocale(LC_ALL,"PORTUGUESE_BR");                                                  
 
  int chave=1234,senha;
 
  printf("Informe a senha:");
  scanf("%i",&senha);
 
  if(senha==chave){
    printf("Acesso Autorizado");
    }else{
    printf("Acesso Negado");
  }
}
