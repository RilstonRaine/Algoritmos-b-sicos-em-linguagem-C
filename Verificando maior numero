#include <stdio.h>
#include <stdlib.h>
#include <locale.h>


int main() {
    setlocale(LC_ALL, "PORTUGUESE_BR");


    int num1, num2, num3;


    printf("Insira trÃªs valores:\n");
    scanf("%i %i %i", &num1, &num2, &num3);


    if (num1 >= num2 && num1 >= num3) {
        printf("O maior numero e %i", num1);
    } else if (num2 >= num1 && num2 >= num3) {
        printf("O maior numero e %i", num2);
    } else {
        printf("O maior numero e %i\n", num3);
    }


    return 0;
}
